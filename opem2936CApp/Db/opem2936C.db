record( longout, "$(P):LO:SC:LAM"){
  field( DESC, "Max Velo:")
  field( OUT,  "$(P):LO:LAM PP")
  field( DRVH, "50000")
  field( DRVL, "0")
}
record( longout, "$(P):LO:LAM"){
  field( SDIS, "$(P):LO:LAM:Sync.PACT")
  field( DISV, "1")
  field( DTYP, "stream")
  field( OUT,  "@opem2936C.proto setLambda $(L) $(A)")
}
record( longin, "$(P):LI:LAM"){
  field( DTYP, "stream")
  field( INP,  "@opem2936C.proto getLambda $(L) $(A)")
  field( SCAN, "1 second")
  field( PINI, "1")
  field( FLNK, "$(P):LO:LAM:Sync")
}
record( longout, "$(P):LO:LAM:Sync"){
  field( DOL,  "$(P):LI:LAM NPP")
  field( OMSL, "closed_loop")
  field( OUT,  "$(P):LO:SC:LAM PP")
}
